{
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6,
      "ecmaFeatures": {
          "blockBindings": true,
          "forOf": true,
          "arrowFunctions": true,
          "defaultParams": true,
          "destructuring": true,
          "spread": true,
          "experimentalObjectRestSpread": true
      }
  },
  "rules": {
      "semi": 2
  },
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  }
}
